[{"title": "First Phase ", "author": "ahninjas", "description": "This is my first Doom (2) map. I thought it would be fun to remake my first Doom (2016) map. This is why there are so many unusual features, like double hallways, as it's based on the Snap Map prefab modules. I had to change a few things, to account for Doom's limitations, and also just to experiment with some different ideas. The original Doom (2016) version of this map can be found on Snap Map. The map ID is VYQD8XR5.", "base": "New from scratch", "build_time": "About 2 weeks", "editor_used": "Doom Builder 2", "bugs": "None (that I know of)", "rating_value": "3", "rating_count": "1", "page_visits": "118", "downloads": "14", "creation_date": "2017-09-17", "file_url": "https://www.doomworld.com/files/file/18888-first-phase/?do=download&csrfKey=673c45e3882356bfe6bbc59553e7d3e8", "game": "DoomII", "category": "0-9", "path": "./WADs/DoomII/Original/1pm1_1pm1.wad", "name": "1pm1.zip", "tile_path": "./WADs/DoomII/Processed/1pm1_1pm1_MAP01.txt", "svg_path": "./WADs/DoomII/Processed - Vectorized/1pm1_1pm1_MAP01.svg"}, {"title": "3 Entradas ", "author": "The Nightmare", "description": "A simple level that takes place in a chemical plant. This is the first level of my mega-wad KAOS.", "base": "It was published before, but now it has new features! :)", "build_time": "I don't remember.", "editor_used": "Wadauthor, I don't know what version.", "bugs": "None known.", "rating_value": "2", "rating_count": "11", "page_visits": "31", "downloads": "12", "creation_date": "2010-06-26", "file_url": "https://www.doomworld.com/files/file/16124-3-entradas/?do=download&csrfKey=a694f48ffce9db97cfb54beff1383153", "game": "DoomII", "category": "0-9", "path": "./WADs/DoomII/Original/3entrad_3entrad.wad", "name": "3entrad.zip", "tile_path": "./WADs/DoomII/Processed/3entrad_3entrad_E1M1.txt", "svg_path": "./WADs/DoomII/Processed - Vectorized/3entrad_3entrad_E1M1.svg"}, {"title": "000 EMERGANCY ", "author": "Paul Corfiatis", "descrip